// Place your key bindings in this file to override the defaults
[
  {
    "command": "workbench.action.gotoLine",
    "key": "cmd+g",
    "when": "editorTextFocus"
  },
  {
    "command": "editor.action.startFindReplaceAction",
    "key": "cmd+r",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "command": "workbench.action.replaceInFiles",
    "key": "shift+cmd+r",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "command": "workbench.files.action.showActiveFileInExplorer",
    "key": "cmd+e",
    "when": "editorFocus"
  },
  {
    "command": "workbench.action.focusActiveEditorGroup",
    "key": "cmd+e",
    "when": "!editorFocus"
  },
  {
    "command": "actions.find",
    "key": "cmd+f",
    "when": "editorTextFocus"
  },
  {
    "command": "editor.action.commentLine",
    "key": "alt+f",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "command": "workbench.action.navigateToLastEditLocation",
    "key": "alt+h",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "command": "workbench.action.navigateBack",
    "key": "ctrl+h",
    "when": "canNavigateBack"
  },
  {
    "command": "workbench.action.navigateForward",
    "key": "ctrl+l",
    "when": "canNavigateForward"
  },
  {
    "command": "editor.action.joinLines",
    "key": "alt+;",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "command": "editor.action.deleteLines",
    "key": "alt+d",
    "when": "textInputFocus && !editorReadonly"
  },
  {
    "command": "-deleteRight",
    "key": "ctrl+d",
    "when": "textInputFocus"
  },
  {
    "command": "findWordAtCursor.next",
    "key": "ctrl+j",
    "when": "editorTextFocus"
  },
  {
    "command": "findWordAtCursor.previous",
    "key": "ctrl+k",
    "when": "editorTextFocus"
  },
  {
    "command": "deleteWordRight",
    "key": "alt+l",
    "when": "textInputFocus && !editorReadonly"
  },
  {
    "command": "deleteWordLeft",
    "key": "alt+s",
    "when": "textInputFocus && !editorReadonly"
  },
  {
    "command": "-deleteWordRight",
    "key": "alt+delete",
    "when": "textInputFocus && !editorReadonly"
  },
  {
    "command": "cursorWordEndRight",
    "key": "ctrl+d",
    "when": "textInputFocus"
  },
  {
    "command": "cursorWordEndLeft",
    "key": "ctrl+f",
    "when": "textInputFocus"
  },
  {
    "command": "cursorWordStartRight",
    "key": "ctrl+s"
  },
  {
    "command": "cursorWordEndRight",
    "key": "ctrl+d",
    "when": "textInputFocus"
  },
  {
    "command": "cursorWordEndLeft",
    "key": "ctrl+f",
    "when": "textInputFocus"
  },
  {
    "command": "workbench.action.quickOpen",
    "key": "ctrl+r"
  },
  {
    "command": "cursorWordStartRightSelect",
    "key": "ctrl+shift+s"
  },
  {
    "command": "editor.action.moveLinesDownAction",
    "key": "alt+j",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "command": "-editor.action.moveLinesDownAction",
    "key": "alt+down",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "command": "editor.action.moveLinesUpAction",
    "key": "alt+k",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "command": "-editor.action.moveLinesUpAction",
    "key": "alt+up",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "command": "editor.action.revealDefinition",
    "key": "ctrl+enter",
    "when": "editorHasDefinitionProvider && editorTextFocus"
  },
  {
    "command": "-editor.action.revealDefinition",
    "key": "f12",
    "when": "editorHasDefinitionProvider && editorTextFocus"
  },
  {
    "key": "cmd+e",
    "command": "-actions.findWithSelection"
  },
  {
    "key": "cmd+e",
    "command": "-editor.action.toggleScreenReaderAccessibilityMode",
    "when": "accessibilityHelpIsShown"
  },
  {
    "key": "ctrl+h",
    "command": "-deleteLeft",
    "when": "textInputFocus"
  },
  {
    "key": "alt+w",
    "command": "deleteInsideWord"
  },
  {
    "key": "1",
    "command": "-gitlens.views.scm.grouped.commits",
    "when": "config.gitlens.views.scm.grouped.views.commits && !gitlens:views:scm:grouped:welcome && focusedView == 'gitlens.views.scm.grouped'"
  },
  {
    "key": "7",
    "command": "-gitlens.views.scm.grouped.contributors",
    "when": "config.gitlens.views.scm.grouped.views.contributors && !gitlens:views:scm:grouped:welcome && focusedView == 'gitlens.views.scm.grouped'"
  },
  {
    "key": "shift+alt+h",
    "command": "-references-view.showCallHierarchy",
    "when": "editorHasCallHierarchyProvider"
},
